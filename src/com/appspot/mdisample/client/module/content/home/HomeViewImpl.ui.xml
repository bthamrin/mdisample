<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.background {
			background-color: #444;
		}
		
		.type {
			color: #777;
			margin-left: 24px !important;
			padding-top: 14px !important;
		}
	</ui:style>

	<g:HTMLPanel styleName="{style.background}" height="100%">
		<g:Label styleName="{style.type}">This page is an instance of Home</g:Label>
		<h1>HOME</h1>
		<p>
			<g:Hyperlink targetHistoryToken="!list=List-100">List-100</g:Hyperlink>
			<g:Hyperlink targetHistoryToken="!list=List-200">List-200</g:Hyperlink>
			<g:Hyperlink targetHistoryToken="!list=List-300">List-300</g:Hyperlink>
		</p>
	</g:HTMLPanel>

</ui:UiBinder> 